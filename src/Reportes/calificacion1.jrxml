	<queryString>
		<![CDATA[select c.curp, a.apellido_paterno, a.apellido_materno, a.nombre, gd.descripcion as grado, gp.letra, a.id_grupo, c.calif1, 
c.calif2, c.calif3, c.calif4, c.calif5, c.calif6, c.calif7, c.calif8, c.calif9, m.descripcion as mes, p.trimestre, round(((c.calif1+c.calif2+c.calif3+c.calif4+c.calif5+c.calif6+c.calif7+c.calif8+c.calif8)/9),2) as promedio
from calificaciones1 as c, alumno as a, mes as m, periodo as p, grado as gd, grupo as gp
where c.id_mes = m.id_mes and c.id_periodo = p.id_periodo and a.id_grado = gd.id_grado and a.id_grupo = gp.id_grupo 
and c.curp = a.curp and c.curp = $P!{curp_entrada}
;]]>
	</queryString>
